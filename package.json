{
  "name": "@unocss-preset-useful/monorepo",
  "type": "module",
  "version": "0.2.2",
  "private": true,
  "packageManager": "pnpm@8.7.4",
  "scripts": {
    "build": "pnpm -r -F=./packages/* run build",
    "stub": "pnpm -r -F=./packages/* --parallel run stub",
    "dev": "pnpm stub",
    "release": "pnpm readme && pnpm lint:fix && bumpp --commit --push --tag",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest --update",
    "play": "pnpm -C playground run dev",
    "postinstall": "pnpm magicAnimate",
    "readme": "esno scripts/readme.ts",
    "magicAnimate": "esno scripts/magicAnimate.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@babel/types": "^7.22.11",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.9",
    "@types/postcss-js": "^4.0.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "typescript": "5.1.6",
    "unbuild": "^2.0.0",
    "unocss": "latest",
    "unocss-preset-useful": "workspace:*",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  }
}
