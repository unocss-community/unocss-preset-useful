{
  "name": "unocss-preset-useful",
  "version": "0.0.7",
  "description": "My useful preset about unocss usage",
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-useful"
  ],
  "license": "MIT",
  "author": "chris-zhu <https://github.com/chris-zhu>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-zhu/unocss-preset-useful.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest --update"
  },
  "bugs": {
    "url": "https://github.com/chris-zhu/unocss-preset-useful/issues"
  },
  "homepage": "https://github.com/chris-zhu/unocss-preset-useful#readme",
  "dependencies": {
    "@unocss/preset-mini": "^0.48.0"
  },
  "devDependencies": {
    "@babel/types": "^7.20.7",
    "@chris-zhu/eslint-config": "^1.0.7",
    "@types/node": "^18.11.18",
    "@unocss/core": "^0.48.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "unbuild": "^1.0.2",
    "unocss": "^0.48.0",
    "vite": "^4.0.3",
    "vitest": "^0.26.3"
  }
}
